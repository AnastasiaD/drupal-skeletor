{
  "type": "interpreter-config",
  "configurations": [
    {
      "settings": [
        {
          "driverOptions": {
            "host": "ondemand.saucelabs.com",
            "port": 80
          },
          "browserOptions": {
            "browserName": "${SAUCE_BROWSER}",
            "build": "${TRAVIS_BUILD_NUMBER}",
            "tags": [
              "${TRAVIS_BRANCH}",
              "${TRAVIS_COMMIT}",
              "${TRAVIS_JOB_NUMBER}",
              "${TRAVIS_JOB_ID}"
            ],
            "passed": "true",
            "username": "${SAUCE_USERNAME}",
            "accessKey": "${SAUCE_ACCESS_KEY}"
          }
        }
      ],
      "scripts": [
        "selenium/tests/*"
      ]
    }
  ]
}
